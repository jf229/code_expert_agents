# This file is an example.
# Create a copy of this file named .env and add your configuration for the providers you want to use.

# LLM Provider Selection
LLM_PROVIDER=ollama

# Ollama Configuration (local or remote)
OLLAMA_ENDPOINT=http://clutchbox.local:11434
OLLAMA_MODEL=granite3.2:8b

# Watson Code Assistant API Key
WCA_API_KEY=YOUR_WCA_API_KEY_HERE

# OpenAI API Key (for OpenAI provider)
OPENAI_API_KEY=YOUR_OPENAI_API_KEY_HERE

# Anthropic API Key (for Claude provider)
ANTHROPIC_API_KEY=YOUR_ANTHROPIC_API_KEY_HERE

# Google API Key (for Gemini provider)
GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY_HERE
